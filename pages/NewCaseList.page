<apex:page controller="NewCaseListController">
    <apex:pageBlock title="New Cases">
        <apex:repeat value="{!newCases}" var="case">
            
            <apex:outputLink value="/{!case.Id}">
                <apex:outputField value="{!case.CaseNumber}"/><br/>
            </apex:outputLink>
            
        </apex:repeat>
    </apex:pageBlock> 
   

</apex:page>